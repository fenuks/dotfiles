// Place your settings in this file to overwrite the default settings
{
    "editor.fontFamily": "'Fira Code', 'Hack', 'Source Code Pro', 'Droid Sans Mono', 'Courier New', monospace, 'Droid Sans Fallback'",
    "telemetry.enableTelemetry": false,
    "extensions.showRecommendationsOnlyOnDemand": true,
    "[cpp]": {
        "editor.quickSuggestions": true
    },
    "[c]": {
        "editor.quickSuggestions": true
    },
    "vim.easymotion": true,
    "vim.hlsearch": true,
    "vim.history": 500,
    "vim.enableNeovim": true,
    "vim.visualstar": true,
    "vim.leader": ",",
    "vim.insertModeKeyBindings": [
        {
            "before": ["j", "k"],
            "after": ["<Esc>"]
        }
    ],
    "editor.wordWrap": "on",
    "editor.fontLigatures": true,
    "files.watcherExclude": {
        "**/bower_components/**": true,
        "**/env/**": true,
        "**/.env/**": true,
        "**/.git/objects/**": true,
        "**/.hg/**": true,
        "**/node_modules/**": true,
        "tags": true,
        "**/volumes/**": true
    },
    "files.exclude": {
        "**/*.a": true,
        "**/*.bak": true,
        "**/.cache": true,
        "**/*.class": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        ".env": true,
        "env": true,
        "**/.git": true,
        "**/.hg": true,
        ".idea": true,
        "**/*.lib": true,
        "**/.mypy_cache": true,
        "**/*.obj": true,
        "**/*.pyc": true,
        "**/__pycache__": true,
        ".ropeproject": true,
        "**/*.so": true,
        "**/.svn": true,
        "tags": true,
        ".sonarlint": true,
        "**/volumes/**": true
    },
    "tslint.jsEnable": true,
    "files.eol": "\n",
    "java.errors.incompleteClasspath.severity": "ignore",
    "files.associations": {
        "*.jsm": "javascript"
    },
    "C_Cpp.intelliSenseEngine": "Default",
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true,
        "suppressUpdateNotice": false,
        "suppressWelcomeNotice": true
    },
    "gitlens.keymap": "chorded",
    "java.jdt.ls.vmargs": "-noverify -Xmx1G -XX:+UseG1GC -XX:+UseStringDeduplication -javaagent:\"/usr/share/java/lombok/lombok.jar\" -Xbootclasspath/a:\"/usr/share/java/lombok/lombok.jar\" -javaagent:\"/home/rkozicki/.vscode/extensions/GabrielBB.vscode-lombok-0.9.2/server/lombok.jar\" -Xbootclasspath/a:\"/home/rkozicki/.vscode/extensions/GabrielBB.vscode-lombok-0.9.2/server/lombok.jar\"",
    "gitlens.advanced.telemetry.enabled": false,
    "sonarlint.disableTelemetry": true,
    "javascript.implicitProjectConfig.checkJs": true,
    "python.jediEnabled": false,
    "editor.smoothScrolling": true,
    "update.enableWindowsBackgroundUpdates": false,
    "window.restoreFullscreen": true,
    "files.trimTrailingWhitespace": true,
    "debug.inlineValues": true,
    "css.lint.compatibleVendorPrefixes": "warning",
    "css.lint.duplicateProperties": "warning",
    "css.lint.ieHack": "warning",
    "css.lint.universalSelector": "warning",
    "css.lint.unknownVendorSpecificProperties": "warning",
    "css.lint.zeroUnits": "warning",
    "hg.useBookmarks": true,
    "hg.commandMode": "server",
    "python.autoComplete.addBrackets": true,
    "latex-workshop.view.pdf.viewer": "tab",
    "search.usePCRE2": true,
    "githubPullRequests.telemetry.enabled": false,
    "vim.neovimPath": "/usr/bin/nvim",
    "atlascode.jira.workingSite": {
        "baseUrlSuffix": "atlassian.net"
    },
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "languageServerHaskell.useCustomHieWrapper": true,
    "languageServerHaskell.useCustomHieWrapperPath": "/usr/bin/hie-wrapper",
    "telemetry.enableCrashReporter": false,
    "sqltools.telemetry": false,
    "rest-client.enableTelemetry": false
    // "editor.formatOnType": true,
    // "editor.formatOnPaste": true,
    // "editor.autoIndent": true
}
